@article{wassink_2021_does,
  title = {Does {{Parental Migration During Childhood Affect Children}}'s {{Lifetime Educational Attainment}}? Evidence {{From Mexico}}},
  shorttitle = {Does {{Parental Migration During Childhood Affect Children}}'s {{Lifetime Educational Attainment}}?},
  author = {Wassink, Joshua Thomas and Viera, Janelle Ashley},
  year = {2021},
