@article{rumbaut_1997a,
  title = {Introduction: Immigration and {{Incorporation}}},
  shorttitle = {Introduction},
  author = {Rumbaut, Rub{\'e}n G.},
  year = {1997},
  month = sep,
  journal = {Sociological Perspectives},
  volume = {40},
