@article{campbell_2009,
  title = {Distinguishing the {{Power}} of {{Agency}} from {{Agentic Power}}: A {{Note}} on {{Weber}} and the ``{{Black Box}}'' of {{Personal Agency}}},
  shorttitle = {Distinguishing the {{Power}} of {{Agency}} from {{Agentic Power}}},
  author = {Campbell, Colin},
  year = {2009},
  month = dec,
  journal = {Sociological Theory},
  volume = {27},
