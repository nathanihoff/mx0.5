@techreport{hartman_2019,
  type = {Working {{Paper}}},
  title = {Generalizing {{Experimental Results Prepared}} for {{Advances}} in {{Experimental}}},
  author = {Hartman, Erin},
  year = {2019},
  pages = {37},
  abstract = {Experiments have come to be a widely accepted and highly regarded method for political science research. Randomization allows for well identified causal effects that are ``internally valid'' to the experimental sample. However, political scientists are driven by asking big questions with broad impacts, and we are rarely able to conduct the ideal experiment on a representative sample of the target population of interest. This leaves open the question: under what conditions can our experimental findings be generalized to a target population of inference, one upon which we did not conduct our experiment. This problem of ``external validity'' has seen increased attention in statistics and the broader social sciences as researchers and policy makers wish to know the impact of interventions beyond the experimental sample. This chapter examines the assumptions researchers must make in order to generalize their experimental finding, including how to define the target population of inference, how differences in treatment administration matter, and the roles of sample selection and treatment effect moderation. Each of these considerations has implications for how researchers should design their experiment if they wish to maximize generalizability of their findings. The chapter then considers techniques for statistical adjustment of experimental results.},
  langid = {english},
  annotation = {No Citation Data},
