@article{gregson_2016,
  title = {Doing the `Dirty Work' of the Green Economy: Resource Recovery and Migrant Labour in the {{EU}}},
  shorttitle = {Doing the `Dirty Work' of the Green Economy},
  author = {Gregson, Nicky and Crang, Mike and Botticello, Julie and Calestani, Melania and Krzywoszynska, Anna},
