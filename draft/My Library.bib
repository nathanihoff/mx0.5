  keywords = {education,inequality,intergenerational mobility,notes,selection bias},
  annotation = {00020},
  file = {/Users/nathan/Google Drive/Zotero PDFs/Zhou (2019) Equalization or Selection - Reassessing the ‚ÄúMeritocratic Power‚Äù of a College Degree in Intergenerational Income Mobility2.pdf}
}

