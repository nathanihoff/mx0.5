  author = {Pfeffer, Fabian T. and Killewald, Alexandra},
  year = {2018},
  month = jun,
  journal = {Social Forces},
  volume = {96},
  number = {4},
  pages = {1411--1442},
  issn = {0037-7732},
  doi = {10.1093/sf/sox086},
  abstract = {Inequality in family wealth is high, yet we know little about how much and how wealth inequality is maintained across generations. We argue that a long-term perspective reflective of wealth's cumulative nature is crucial to understand the extent and channels of wealth reproduction across generations. Using data from the Panel Study of Income Dynamics that span nearly half a century, we show that a one-decile increase in parents' wealth position is associated with an increase of about four percentiles in their offspring's wealth position in adulthood. We show that grandparental wealth is a unique predictor of grandchildren's wealth, above and beyond the role of parental wealth, suggesting that a focus on only parent-child dyads understates the importance of family wealth lineages. Second, considering five channels of wealth transmission\textemdash gifts and bequests, education, marriage, homeownership, and business ownership\textemdash we find that most of the advantages arising from family wealth begin much earlier in the life course than the common focus on bequests implies, even when we consider the wealth of grandparents. We also document the stark disadvantage of African American households in terms of not only their wealth attainment but also their intergenerational wealth mobility compared to whites.},
  langid = {english},
  keywords = {notes},
  annotation = {00040},
  file = {/Users/nathan/Google Drive/Zotero PDFs/Pfeffer & Killewald (2018) Generations of Advantage. Multigenerational Correlations in Family Wealth.pdf}
}

@article{pfeffer_2021_wealth,
  title = {The {{Wealth Inequality}} of {{Nations}}},
  author = {Pfeffer, Fabian T. and Waitkus, Nora},
  year = {2021},
  month = aug,
  journal = {American Sociological Review},
  volume = {86},
  number = {4},
  pages = {567--602},
  publisher = {{SAGE Publications Inc}},
  issn = {0003-1224},
  doi = {10.1177/00031224211027800},
  abstract = {Comparative research on income inequality has produced several frameworks to study the institutional determinants of income stratification. In contrast, no such framework and much less empirical evidence exist to explain cross-national differences in wealth inequality. This situation is particularly lamentable as cross-national patterns of inequality in wealth diverge sharply from those in income. We seek to pave the way for new explanations of cross-national differences in wealth inequality by tracing them to the influence of different wealth components. Drawing on the literatures on financialization and housing, we argue that housing equity should be the central building block of the comparative analysis of wealth inequality. Using harmonized data on 15 countries included in the Luxembourg Wealth Study (LWS), we demonstrate a lack of association between national levels of income and wealth inequality and concentration. Using decomposition approaches, we then estimate the degree to which national levels of wealth inequality and concentration relate to cross-national differences in wealth portfolios and the distribution of specific asset components. Considering the role of housing equity, financial assets, non-housing real assets, and non-housing debt, we show that cross-national variation in wealth inequality and concentration is centrally determined by the distribution of housing equity.},
  langid = {english},
  keywords = {_tablet_modified,comparison,housing,income,inequality,wealth},
  annotation = {00009},
  file = {/Users/nathan/Google Drive/Zotero PDFs/2021. Pfeffer & Waitkus. The Wealth Inequality of Nations.pdf}
}

@article{phalet_2001,
  title = {Intergenerational {{Transmission}} in {{Turkish Immigrant Families}}: Parental {{Collectivism}}, {{Achievement Values}} and {{Gender Differences}}},
  shorttitle = {Intergenerational {{Transmission}} in {{Turkish Immigrant Families}}},
  author = {Phalet, Karen and Sch{\"o}npflug, Ute},
  year = {2001},
  journal = {Journal of Comparative Family Studies},
  volume = {32},
  number = {4},
  pages = {489--504},
  issn = {0047-2328},
  abstract = {This study investigates the process of value transmission in Turkish immigrant families. Intergenerational transmission is the process that provides cultural continuity. Under contextual conditions of migration, however, rapid and deep cultural changes occur. Major research questions regarding transmission in these special circumstances were (1) the selective contents, (2) the effective means and (3) the differential intensity of value transmission across gender. More specifically, this study aims at clarifying these issues: Which parental values are most readily transmitted? Is value transmission mediated or enhanced by the intended socialisation goals of Turkish immigrant parents? Does intensity of value transmission vary as a function of gender and school careers of the children? To address these questions Lisrel-type (multi-sample latent-level) causal models of value transmission were tested for 400 parent-child dyads from Turkish families in Germany. Applying Campbell's (1975) theory of group survival to the development of family-oriented achievement values in Turkish youth, it was expected and found that parental collectivism and achievement values \textemdash{} not individualism \textemdash{} are effectively transmitted. In line with Kagitcibasi's (1996) model of family change, value transmission was mediated by intended socialisation goals, so that collectivistic and aspiring Turkish immigrants parents stress conformity and achievement goals, though only conformity goals appeared to enhance effective transmission. Finally, the transmission models were fully replicated for male and female dyads, but Turkish immigrant parents were found to stress achievement values more for children in non-vocational tracks as opposed to children in vocation school tracks and for sons as compared to daughters. The role of value transmission for family-based adaptation strategies in Turkish migration is discussed. Cette investigation \'etudie le processus de transmission de valeurs en families turques immigr\'ees. Transmission interg\'en\'erationale est le proc\`es qui garantie la continuit\'e interg\'en\'erationale. Cependent en migration des changes rapides et intenses apparaissent. Plusieurs questions de recherche importantes en cettes circonstances particuli\`eres \'etaient: (1) la s\'election des contenues de transmission, (2) des moins effectifs pour transmettre, (3) l'intensit\'e diff\'erente de la transmission des valeurs pour les deux sexes. Plus d\'etaill\'e, l'\'etude a pour but \`a clarifier les probl\`emes suivantes: Quels valeurs parentaux ont la plus grande probabilit\'e \`a \^etre transmis ? La transmission est-elle etiforc\'ee par les buts de socialisation des parents turques immigr\'es ? Est-ce que l'intensit\'e de la transmission des valeurs diff\`ere en fonction du sexe et carri\`ere \'educative des enfants? Les questions \'etaient adress\'ees par des analyses de type Lisrel (multigroupes latent level) avec 400 dyades de parent et enfant des families turques en Allemagne. L'hypoth\`ese de Campbell (1975) de la maintenance de groupe est appliqu\'ee au d\'eveloppement des valeurs d'ach\`evement orient\'es \`a la famille chez adolescents turques: II s'\'etait attendu et trouv\'e que collectivisme parentale et valeurs d'ach\`evement \'etaient effectivement transmis - mais pas individualisme. Concordant au mod\`ele de changement familial de Kagitcibasi (1996), la transmission des valeurs \'etaient enforc\'ee par les buts de socialisation: les parents turques immigr\'es, qui \'etaient collectivistiques et aspirants, mettaient l'accent sur les buts de conformit\'e et ach\`evement, mais seulement le but de conformit\'e semblait enforcer la transmission effective. Finalement les mod\`eles de transmission \'etaient r\'epliqu\'es compli\`etement pour des pairs de parent-enfant maies et f\'eminins. Les parents turques accentuaient le valeur de l'ach\`evement plus pour leurs enfants en les \'ecoles g\'en\'erales que pour les enfants en \'ecoles professionnelles et plus pour leurs fils que pour leurs filles. Le r\^ole de transmission des valeurs pour les strat\'egies de l'adaptation familiale en migration turque est discut\'e. Este estudio investiga el proceso de transmisi\'on de los valores en las familias turcas imigrantes. La transmisi\'on intergeneracional es el proceso que provee continuidad cultural. Bajo cualquier condici\'on del contexto de imigraci\'on, ocurren r\'apidos y profundos cambios culturales. Fueron realizadas preguntas importantes de investigaci\'on acerca de dicha transmisi\'on en estas espec\'ialos cirscuristancias: (1) los contenidos selectivos, (2) los medios efectivos y (3) la intensidad difereticial del valor entre los sexos. M\'as espec\'ificamente, este estudio pretende clarificar estos temas: Cu\'ales valores paternos son realmente transmitidos? Es la transmisi\'on de valores de los objetivos de la socializaci\'on de los padres turcos imigi-atites facilitada? La intensidad de transmisi\'on de valores varia en funci\'on del sexo y carrera escolar de los ni\~nos? Para direccionar estas preguntas Tipo Lisrel (gruposm\'ultiples nivel-latente) varios modelos causales de transmisi\'on de valores, fueron examinados 400 pares de padres-ni\~nos de familias turcas en Alemania. Aplicando la teor\'ia de Campbell's (1975) de supervivencia de grupos al desarrollo del valor del rendimiento en la orientaci\'on familiar en la juventud turca fue esperado y encontrado que el colectivismo paternal y valor del rendimiento \textemdash{} pero no valores individualistas \textemdash{} son efectivamente transmitidos. Concordando con el modelo de Kagitcibasi's (1996) de cambios en la familiar la transmisi\'on de valores fue facilitada con intentos de objetivos de socializaci\'on. Esto significa que padres turcos imigrantes colectivistas y aspirantes asent\'uan objetivos de conformidad y objetivos de rendimiento; pero solo los objetivos de conformidad parecen facilitar la transmisi\'on efectiva.. Finalmente los modelos de transmisi\'on fueron totalmente repetidos por pares de hombres (padre e hijo) y mujeres (madre e hija) ; pero los padres imigrantes turcos acent\'uan los valores de rendimiento m\'as en los adolecentes de colegios regulares y menos en los adolecentes de colegios de entrenamiento profesional, mas en los hombres que en las muieres. 'El rol de transmisi\'on de valores de las estrategias de adaptaci\'on de la base familiar en la imigraci\'on turca es discutida},
  annotation = {00134},
  file = {/Users/nathan/Google Drive/Zotero PDFs/Phalet & Sch√∂npflug (2001) Intergenerational Transmission in Turkish Immigrant Families.pdf}
}

