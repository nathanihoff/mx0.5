@article{fox_2015,
  title = {Denying {{Discrimination}}: Status, `{{Race}}', and the {{Whitening}} of {{Britain}}'s {{New Europeans}}},
  shorttitle = {Denying {{Discrimination}}},
  author = {Fox, Jon E. and Moro{\c s}anu, Laura and Szilassy, Eszter},
