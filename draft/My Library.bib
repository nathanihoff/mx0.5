@article{dekeyser_2011,
  title = {Self-Reported Mental Health in 12-Year-Old Second-Generation Immigrant Children in {{Sweden}}},
  shorttitle = {Self-Reported Mental Health in 12-Year-Old Second-Generation Immigrant Children in {{Sweden}}},
  author = {Dekeyser, L. and Svedin, C. G. and Agnafors, S. and Sydsjo, G.},
