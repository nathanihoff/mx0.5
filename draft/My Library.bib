@article{seltzer_2019,
  title = {Beyond the {{Great Recession}}: Labor {{Market Polarization}} and {{Ongoing Fertility Decline}} in the {{United States}}},
  shorttitle = {Beyond the {{Great Recession}}},
  author = {Seltzer, Nathan},
  year = {2019},
  month = aug,
  journal = {Demography},
  volume = {56},
  number = {4},
  pages = {1463--1493},
  issn = {1533-7790},
  doi = {10.1007/s13524-019-00790-6},
  abstract = {In the years since the Great Recession, social scientists have anticipated that economic recovery in the United States, characterized by gains in employment and median household income, would augur a reversal of declining fertility trends. However, the expected post-recession rebound in fertility rates has yet to materialize. In this study, I propose an economic explanation for why fertility rates have continued to decline regardless of improvements in conventional economic indicators. I argue that ongoing structural changes in U.S. labor markets have prolonged the financial uncertainty that leads women and couples to delay or forgo childbearing. Combining statistical and survey data with restricted-use vital registration records, I examine how cyclical and structural changes in metropolitan-area labor markets were associated with changes in total fertility rates (TFRs) across racial/ethnic groups from the early 1990s to the present day, with a particular focus on the 2006\textendash 2014 period. The findings suggest that changes in industry composition\textemdash specifically, the loss of manufacturing and other goods-producing businesses\textemdash have a larger effect on TFRs than changes in the unemployment rate for all racial/ethnic groups. Because structural changes in labor markets are more likely to be sustained over time\textemdash in contrast to unemployment rates, which fluctuate with economic cycles\textemdash further reductions in unemployment are unlikely to reverse declining fertility trends.},
  langid = {english},
  keywords = {notes},
  annotation = {00013},
  file = {/Users/nathan/Google Drive/Zotero PDFs/Seltzer (2019) Beyond the Great Recession - Labor Market Polarization and Ongoing Fertility Decline in the United States.pdf}
}

